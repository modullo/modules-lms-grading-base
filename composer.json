{
    "name": "modullo/modules-lms-grading-base",
    "description": "description",
    "license": "GPL-3.0-or-later",
    "authors": [
      {
        "name": "Musah Musah",
        "email": "shehialiyu0@gmail.com"
      }
    ],
    "require": {
      "php": ">=7.3",
      "aws/aws-sdk-php": "^3.74",
      "laravel/framework": "^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.5"
    },
    "config": {
      "optimize-autoloader": true,
      "sort-packages": true
    },
    "autoload": {
      "psr-4": {
        "Modullo\\ModulesLmsGradingBase\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Modullo\\ModulesLmsGradingBase\\ModulesLmsGradingBaseServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
  
      "post-install-cmd": [
        "Illuminate\\Foundation\\ComposerScripts::postInstall",
        "php artisan optimize",
        "php artisan vendor:publish"
      ],
      "post-update-cmd": [
        "php artisan optimize",
        "php artisan vendor:publish"
      ]
    }
  }
  